                    android:layout_gravity="bottom|center"
                    android:elevation="10dp"
                    android:layout_height="@dimen/_119pxv"
                    android:layout_marginEnd="@dimen/_118pxh"
                    android:layout_marginStart="@dimen/_118pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:padding="15dp"
                    android:layout_width="@dimen/_124pxh">
                    <ImageView
                        android:id="@+id/profile_img"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        tools:src="@drawable/img_rating"
                        />
                </LinearLayout>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearRowarrowright"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_66pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadiusnew">
                <LinearLayout android:id="@+id/linearRowrating"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.94"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameStackrating"
                        android:layout_height="@dimen/_48pxv"
                        android:layout_width="@dimen/_65pxh" style="@style/groupStylelime_100_bfcornerRadius">
                        <ImageView android:id="@+id/imageRating"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_45pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_45pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rating"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rating"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRateUs"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_60pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.48"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_rate_us"
                        style="@style/txtPoppinsmedium16" tools:text="@string/lbl_rate_us"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_93pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowarrowrightOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadiusnew">
                <LinearLayout android:id="@+id/linearRowpaper"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameStackpaper"
                        android:layout_height="@dimen/_48pxv"
                        android:layout_width="@dimen/_65pxh" style="@style/groupStylelime_100_bfcornerRadius">
                        <ImageView android:id="@+id/imagePaper"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginEnd="@dimen/_13pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="@dimen/_40pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_paper"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_paper"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtWhitePaper"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_44pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_weight="0.59"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_whitepaper"
                        style="@style/txtPoppinsmedium16" tools:text="@string/lbl_whitepaper"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowrightOne"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_76pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowarrowrightTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadiusnew">
                <LinearLayout android:id="@+id/linearRowjourney"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.94"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameStackjourney"
                        android:layout_height="@dimen/_48pxv"
                        android:layout_width="@dimen/_65pxh" style="@style/groupStylelime_100_bfcornerRadius">
                        <ImageView android:id="@+id/imageJourney"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_45pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_45pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_journey"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_journey"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRoadmap"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_weight="0.55"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_roadmap"
                        style="@style/txtPoppinsmedium16" tools:text="@string/lbl_roadmap"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowrightTwo"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_21pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_83pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowarrowrightThree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadiusnew">
                <LinearLayout android:id="@+id/linearRowrectangleeleven"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.94"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameStackjourneykyc"
                        android:layout_height="@dimen/_48pxv"
                        android:layout_width="@dimen/_65pxh" style="@style/groupStylelime_100_bfcornerRadius">
                        <ImageView android:id="@+id/imageJourneykyc"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_45pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_45pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/video_id_verification"
                            tools:ignore="ContentDescription" tools:src="@drawable/video_id_verification"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtKyc"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_76pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_weight="0.32"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_kyc"
                        style="@style/txtPoppinsmedium16" tools:text="@string/lbl_kyc"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowrightThree"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_108pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowarrowrightFour"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadiusnew">
                <LinearLayout android:id="@+id/linearRowprivacypolicy"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <FrameLayout
                        android:id="@+id/frameStackprivacypolicy"
                        android:layout_height="@dimen/_48pxv"
                        android:layout_width="@dimen/_65pxh" style="@style/groupStylelime_100_bfcornerRadius">
                        <ImageView android:id="@+id/imagePrivacyPolicy"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_45pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_45pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_privacypolicy"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_privacypolicy"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrivacyPolicyOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_35pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_weight="0.63"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_privacy_policy"
                        style="@style/txtPoppinsmedium16" tools:text="@string/lbl_privacy_policy"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowrightFour"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_68pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowrectanglethirteen"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:padding="@dimen/_5pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout
                    android:gravity="center_horizontal|center_vertical"
                    android:orientation="horizontal"
                    android:elevation="25dp"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_15pxv"
                    android:background="@drawable/rectangle_bg_white_a700_radius_15"
                    android:id="@+id/viewRectangleThirteen"
                    android:layout_height="@dimen/_45pxv" android:layout_width="@dimen/_69pxh">
                    <ImageView
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                       android:src="@drawable/telegram"/>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center_horizontal|center_vertical"
                    android:orientation="horizontal"
                    android:elevation="25dp"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:background="@drawable/rectangle_bg_white_a700_radius_15"
                    android:id="@+id/viewRectangleFifteen"
                    android:layout_height="@dimen/_45pxv"
                    android:layout_marginStart="@dimen/_53pxh" android:layout_width="@dimen/_69pxh">
                    <ImageView
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:src="@drawable/twitter"/>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center_horizontal|center_vertical"
                    android:orientation="horizontal"
                    android:background="@drawable/rectangle_bg_white_a700_radius_15"
                    android:id="@+id/viewRectangleFourteen"
                    android:elevation="25dp"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_height="@dimen/_45pxv"
                    android:layout_marginStart="@dimen/_54pxh" android:layout_width="@dimen/_69pxh">
                    <ImageView
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:src="@drawable/youtube"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center"
            android:background="#FFFFFF"
            android:gravity="center"
            app:layout_behavior="com.google.android.material.behavior.HideBottomViewOnScrollBehavior">

            <com.google.android.material.tabs.TabLayout
                android:id="@+id/tabLayout"
                android:layout_width="wrap_content"
                android:layout_height="72dp"
                android:layout_gravity="center"
                app:tabGravity="center"
                app:tabIconTint="@android:color/darker_gray"
                app:tabIndicator="@null"
                app:tabMinWidth="40dp"
                app:tabMode="scrollable"
                app:tabRippleColor="@null">

                <com.google.android.material.tabs.TabItem
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:icon="@drawable/ic_categories" />

                <com.google.android.material.tabs.TabItem
                    android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                    android:icon="@drawable/ic_home" />

           </com.google.android.material.tabs.TabLayout>

        </LinearLayout>
    </FrameLayout>
</layout>
