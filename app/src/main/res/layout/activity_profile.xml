        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnrectangleone"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_6pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackrectangleone"
                android:layout_height="@dimen/_210pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangleOne"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_182pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle1"/>
                <LinearLayout
